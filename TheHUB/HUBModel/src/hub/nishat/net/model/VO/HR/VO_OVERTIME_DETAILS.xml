  <SQLQuery>
    <![CDATA[SELECT EO_Attendance.ATTENDANCE_DATE,
       EO_Attendance.EMP_ATD_ID, 
       EO_Attendance.EMP_ID, 
       EO_Attendance.EMP_NAME,
       EO_Attendance.MAX_OUT_TIME,
       EO_Attendance.MIN_IN_TIME,
       to_char(EO_Attendance.MIN_IN_TIME,'hh:mi am') in_time,
       to_char(EO_Attendance.MAX_OUT_TIME,'hh:mi am') out_time
       ,cal.description
       ,CAL.DAY_OF_WEEK
       ,to_char(EO_Attendance.attendance_Date,'mm') month_number
       ,floor(((EO_Attendance.MAX_OUT_TIME - EO_Attendance.MIN_IN_TIME ) * 24 *60)/60)||':'||mod(((EO_Attendance.MAX_OUT_TIME - EO_Attendance.MIN_IN_TIME ) * 24 *60),60) time_worked
FROM XX_E_PORTAL_EMP_ATD EO_Attendance
,xx_e_portal_leave_calander cal
where  EO_Attendance.emp_type = '4' --GUARDS
and cal.transaction_date = EO_Attendance.attendance_date
and (CAL.DAY_OF_WEEK = 'SUN'  or cal.description not like  '%SATURDAY%'  )
and EO_Attendance.effective_worked_hours is not null
ORDER BY EO_Attendance.EMP_ID
,EO_Attendance.attendance_date]]>
  </SQLQuery>
