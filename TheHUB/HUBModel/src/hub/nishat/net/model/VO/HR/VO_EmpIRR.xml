  <SQLQuery>
    <![CDATA[SELECT 
distinct 
irr.IRREGULARITY_ID,
org.full_name_parent,
       irr.TYPE,
       irr.attendance_date,
       irr.exception_remarks,
       irr.MINUTES_MISSING,
       irr.emp_id,
       TO_CHAR (irr.attendance_date, 'MM') || TO_CHAR (irr.attendance_date, 'yyyy')
          month_code,
       irr.exception_approved
       ,er.line_manager_comments
  FROM xx_e_portal_emp_irregularities irr,
            xx_employee_org_chart_V org
            ,xx_e_portal_exception_requests er 
 WHERE exception_approved IS NOT NULL
 and irr.emp_id = org.child_person_id
 and org.parent_person_id is not null
and  er.irregularity_id = irr.irregularity_id]]>
  </SQLQuery>
