  <SQLQuery>
    <![CDATA[SELECT 
distinct
posting.EMP_POSTED_FLAG,
decode(irr.IRREGULARITY_ID,null,decode(irr.EXCEPTION_APPROVED,null,null),irr.exception_remarks) IsException,
EO_AttendanceHR.EMP_ATD_ID, 
        cal.DESCRIPTION,
        cal.Day_of_Week, 
       EO_AttendanceHR.EMP_ID, 
       EO_AttendanceHR.EMP_NAME, 
       EO_AttendanceHR.CARD_NUM, 
       EO_AttendanceHR.EXPECTED_WORK_HOURS, 
       EO_AttendanceHR.MIN_IN_TIME, 
       EO_AttendanceHR.MAX_OUT_TIME,
       EO_ATTENDANCEHR_V.TOTAL,
       to_char(EO_AttendanceHR.MIN_IN_TIME,'hh:Mi AM') inTime, 
       to_char(EO_AttendanceHR.MAX_OUT_TIME,'hh:mi AM') outTime, 
       EO_AttendanceHR.EFFECTIVE_WORKED_HOURS, 
       EO_AttendanceHR.MAX_START_TIME, 
       EO_AttendanceHR.MAX_END_TIME, 
       EO_AttendanceHR.END_TIME, 
       EO_AttendanceHR.START_TIME, 
       EO_AttendanceHR.ATTENDANCE_DATE,
       to_char(EO_AttendanceHR.ATTENDANCE_DATE,'DAY') Day, 
       EO_AttendanceHR.LEAVE_TYPE_ID, 
       EO_AttendanceHR.LEAVE_APPROVED_FLAG, 
       EO_AttendanceHR.POLICY_EXCEPTION, 
       EO_AttendanceHR.POLICY_EXCEPTION_APPROVED_FLAG, 
       EO_AttendanceHR.IS_HALF,
       EO_AttendanceHR.leave_today,
       EO_AttendanceHR.type_of_leave,
       EO_AttendanceHR.leave_cancelled,
       EO_AttendanceHR.emp_type,
       emp_type.working_time,
       to_char(EO_AttendanceHR.ATTENDANCE_DATE,'MON') month,
       to_char(EO_AttendanceHR.ATTENDANCE_DATE,'MM') monthnumber,
       to_char(EO_AttendanceHR.ATTENDANCE_DATE,'YYYY') year, to_char(EO_AttendanceHR.ATTENDANCE_DATE,'MM')||to_char(EO_AttendanceHR.ATTENDANCE_DATE,'yyyy') month_code
FROM XX_E_PORTAL_EMP_ATD EO_AttendanceHR,
         xx_e_portal_emp_atd_v EO_AttendanceHR_V,
        xx_e_portal_leave_calander cal,
        xx_e_portal_emp_type emp_type,
        xx_e_portal_emp_irregularities irr,
        xx_e_portal_atd_posting posting
where cal.transaction_date = EO_AttendanceHR.attendance_Date
and EO_AttendanceHR.emp_type = emp_type.emp_type_id
and irr.attendance_date (+)=  EO_AttendanceHR.attendance_date
and irr.emp_id (+)= EO_AttendanceHR.emp_id
and to_char(EO_AttendanceHR.emp_id) = EO_AttendanceHR_V.EMP_ID(+)
and   to_char(EO_AttendanceHR.ATTENDANCE_DATE, 'MON-DD-YYYY') = to_char(EO_AttendanceHR_V.IN_TIME(+),'MON-DD-YYYY')
and posting.POSTING_MONTH(+)||posting.POSTING_YEAR(+)= to_char(EO_AttendanceHR.attendance_Date,'Monyyyy')
order by attendance_date asc]]>
  </SQLQuery>
